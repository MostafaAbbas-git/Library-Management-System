{
  "name": "lib_mng_sys",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server",
    "dev": "nodemon --watch src --exec npm run start-dev",
    "start-dev": "nodemon --exec npx ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "jasmine-ts": "cross-env ENV=test  npm run migrate-test-down && npm run migrate-test-up && cross-env ENV=test jasmine-ts",
    "test": "npx tsc && cross-env ENV=test  npm run migrate-test-down && npm run migrate-test-up && cross-env ENV=test jasmine",
    "run_tests": "cross-env ENV=test jasmine",
    "setup-db": "bash ./setup_database.sh",
    "create-dev-db": "db-migrate db:create lib_mng_sys",
    "create-test-db": "db-migrate db:create lib_mng_sys_test",
    "drop-dev-db": "db-migrate db:drop lib_mng_sys",
    "drop-test-db": "db-migrate db:drop lib_mng_sys_test",
    "migrate-dev-up": "cross-env ENV=dev db-migrate --env dev -c 9  up",
    "migrate-dev-down": "cross-env ENV=dev db-migrate --env dev -c 9 down",
    "migrate-test-up": "cross-env ENV=test db-migrate --env test -c 9 up",
    "migrate-test-down": "cross-env ENV=test db-migrate --env test -c 9 down",
    "create-migrations": "db-migrate create users --sql-file",
    "build": "npx tsc",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts"
  },
  "author": "Mostafa Mahmoud",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "sinon": "^17.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^10.0.6",
    "@types/pg": "^8.6.5",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "jasmine": "^3.10.0",
    "jasmine-ts": "^0.4.0",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^5.3.3"
  }
}